{
  "name": "ShortcutKey2URL",
  "description": "Open URL with the shortcut key.",
  "manifest_version": 2,
  "version": "1.0",
  "homepage_url": "https://github.com/onozaty/chrome-shortcutkey2url",
  "icons": {
  },

  "background": {
    "scripts": ["settings.js", "handler.js", "background.js"]
  },

  "permissions": [
    "tabs", "activeTab"
  ],

  "browser_action": {
    "default_title": "ShortcutKey2URL",
    "default_popup": "popup/popup.html"
  },

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Q"
      }
    }
  }

}
